{
  "name": "marknotes-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "release": "standard-version",
    "serve:build": "lite-server --baseDir='dist/marknotes-fe'",
    "build:stats": "NODE_ENV=production ng build --prod --source-map",
    "analyze": "source-map-explorer ./dist/marknotes-fe/*-es2015.*.js",
    "test-ci": "ng test --code-coverage --watch=false --progress=false --browsers=ChromeHeadlessCI"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@types/dompurify": "^2.0.1",
    "@types/highlightjs": "^9.12.0",
    "@types/marked": "^0.7.3",
    "bootstrap": "^4.4.1",
    "dompurify": "^2.0.8",
    "highlight.js": "^10.7.2",
    "marked": "^1.1.0",
    "material-design-icons": "^3.0.1",
    "ngx-build-plus": "^10.1.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/language-service": "~10.2.4",
    "@commitlint/cli": "^8.3.5",
    "@ngrx/store-devtools": "^10.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.2.4",
    "codecov": "^3.7.0",
    "codelyzer": "^5.1.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lite-server": "^2.5.4",
    "postcss": "^8.2.6",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.1.0",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.0.4",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.4.2",
    "standard-version": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^2.0.3",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
